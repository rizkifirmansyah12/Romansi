<?xml version ="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="true">
<shape android:shape="rectangle">
<corners android:radius="30dip"/>
<stroke android:width="10dip" android:color="#FFD033"/>
<gradient android:angle="-90" android:startColor="#FF33EEFF" android:endColor="#FF33EEFF"/>
</shape>
</item>
<item android:state_focused="true">
<shape android:shape="rectangle">
	<corners android:radius="30dip"/>
<stroke android:width="10dip" android:color="#FFBEFF32"/>
<solid android:color="#FFBEFF32"/>
</shape>
</item>
<item>
<shape android:shape="rectangle">
<corners android:radius="30dip"/>
<storke android:width="10dip" android:color="#FFF44336"/>
<gradient android:angle="-90" android:startColor="#FFF44336" android:endColor="#FFF44336"/>
</shape>
</item>
</selector>