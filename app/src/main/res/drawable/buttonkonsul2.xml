<?xml version ="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="true">
<shape android:shape="rectangle">
<corners android:radius="30dip"/>
<stroke android:width="10dip" android:color="#FFD033"/>
<gradient android:angle="-90" android:startColor="#FF0EC0EE" android:endColor="#FF0EC0EE"/>
</shape>
</item>
<item android:state_focused="true">
<shape android:shape="rectangle">
	<corners android:radius="30dip"/>
<stroke android:width="10dip" android:color="#FF09FF01"/>
<solid android:color="#FF09FF01"/>
</shape>
</item>
<item>
<shape android:shape="rectangle">
<corners android:radius="30dip"/>
<storke android:width="10dip" android:color="#FFE101FF"/>
<gradient android:angle="-90" android:startColor="#FFE101FF" android:endColor="#FFE101FF"/>
</shape>
</item>
</selector>